make()
{
    SMBCOMMON_SOURCES="\
        wire_close.c         \
        wire_create_dir.c    \
        wire_create_file.c   \
        wire_datetime.c      \
        wire_delete.c        \
        wire_directory.c     \
        wire_check_dir.c     \
        wire_echo.c          \
        wire_error.c         \
        wire_find_close2.c   \
        wire_flush.c         \
        wire_lockx.c         \
        wire_open.c          \
        wire_negotiate.c     \
        wire_ntrename.c      \
        wire_nttransaction.c \
        packet.c             \
        wire_readx.c         \
        wire_rename.c        \
        wire_session_setup.c \
        wire_setinfo.c       \
        wire_setinfo2.c      \
        wire_transaction.c   \
        wire_tree_connect.c  \
        wire_write.c         \
        wire_writex.c        \
        smbkrb5.c"

    mk_library \
        LIB=lwiosmbcommon \
        SOURCES="$SMBCOMMON_SOURCES" \
        INCLUDEDIRS=". ../include ../../include" \
        HEADERDEPS="openssl/md5.h openssl/hmac.h reg/lwntreg.h lw/base.h krb5.h gssapi.h" \
        LIBDEPS="lwbase regclient crypto gssapi_krb5 lwiocommon krb5 $LIB_NSL $LIB_SOCKET"
}